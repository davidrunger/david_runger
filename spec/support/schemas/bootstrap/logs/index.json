{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Root",
  "definitions": {
    "Root": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "nonce": {
          "type": "string",
          "format": "uuid"
        },
        "current_user": {
          "$ref": "#/definitions/User"
        },
        "alert_toast_messages": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "notice_toast_messages": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "logs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Log"
          }
        },
        "log_input_types": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/LogInputType"
          }
        },
        "log_selector_keyboard_shortcut": {
          "type": "string"
        }
      },
      "required": [
        "log_input_types",
        "log_selector_keyboard_shortcut",
        "logs",
        "nonce"
      ],
      "title": "Root"
    },
    "User": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "email": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      },
      "required": ["email", "id"],
      "title": "User"
    },
    "LogInputType": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "data_type": {
          "type": "string"
        },
        "label": {
          "type": "string"
        }
      },
      "required": ["data_type", "label"],
      "title": "LogInputType"
    },
    "Log": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "data_label": {
          "type": "string"
        },
        "data_type": {
          "type": "string"
        },
        "description": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        },
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        },
        "publicly_viewable": {
          "type": "boolean"
        },
        "reminder_time_in_seconds": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "log_shares": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/User"
          }
        }
      },
      "required": [
        "data_label",
        "data_type",
        "description",
        "id",
        "name",
        "slug",
        "user"
      ],
      "title": "Log"
    }
  }
}
