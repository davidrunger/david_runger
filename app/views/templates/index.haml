%div
  %strong Hi, #{current_user.email}! :)
  = link_to('Sign Out', destroy_user_session_path, method: :delete)
%div
  %h1 Your Templates
  = link_to('+ New template', new_template_path)
  - if current_user.templates.present?
    %ul
    - current_user.templates.each do |template|
      %li= link_to(template.name, template)
